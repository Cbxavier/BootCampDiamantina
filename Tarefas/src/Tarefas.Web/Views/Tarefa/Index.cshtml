@model IEnumerable<Tarefas.Web.Models.TarefaViewModel>
<table class="table table-white">
    <tr>
        <td>
            Titulo
        </td>
        <td>
            Descricao
        </td>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
        <td>
            @item.Titulo
        </td>
        <td>
            @item.Descricao
        </td>

        </tr>
    } 
</table>